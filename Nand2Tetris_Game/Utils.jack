class Utils {

    function int random(int max) 
    {
        var int score;
        var int result;

        let score = GameFiled.getScore();
        let result = 2 * ( score - (((max / 2) + 1) * (score / ((max / 2) + 1))));

        if (result = max)
        {
            let result = result - 2;
        }
        return result;
    }

}
